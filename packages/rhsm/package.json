  },
  "homepage": "https://github.com/redhat-developer/redhat-javascript-rest-clients#readme",
  "devDependencies": {
    "@types/node": "^20.11.10",
    "openapi-typescript-codegen": "^0.27.0",
    "typescript": "^5.3.3"
