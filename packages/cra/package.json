{
  "name": "@redhat-developer/cra-client",
  "version": "0.0.1",
  "description": "Type safe client for Red Hat Container Registry Authorizer (CRA) REST API",
  "main": "./dist/index.js",
  "scripts": {
    "generate": "openapi -c axios -i ./swagger/container-registry-authorizer-1.3.0-swagger-spec.json -o ./src-generated --indent 2 --name ContainerRegistryAuthorizerClient",
    "compile": "tsc -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/redhat-developer/redhat.com-javascript-rest-clients.git"
  },
  "keywords": [
    "REST",
    "typescript",
    "redhat",
    "registry"
  ],
  "author": "Denis Golovin (dgolovin@redhat.com)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/redhat-developer/redhat-javascript-rest-clients/issues"
  },
  "homepage": "https://github.com/redhat-developer/redhat-javascript-rest-clients/cra-client#readme",
  "devDependencies": {
    "@types/node": "^20.11.10",
    "openapi-typescript-codegen": "^0.27.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "form-data": "^4.0.0"
  }
}
